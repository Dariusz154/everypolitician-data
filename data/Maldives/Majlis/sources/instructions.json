{
  "sources": [
    {
      "file": "morph/psephos.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/maldives-psephos",
        "query": "SELECT * FROM data ORDER BY name"
      },
      "source": "http://psephos.adam-carr.net/",
      "generates": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/maldives-representatives-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Maldives/Majlis"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22704045"
      }
    }
  ]
}
