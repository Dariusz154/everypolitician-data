      "type": "person"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/turks-and-caicos-assembly-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": { 
        "incoming_field": "wikipedia__en",
        "existing_field": "wikiname"
      }
    }
  ]
