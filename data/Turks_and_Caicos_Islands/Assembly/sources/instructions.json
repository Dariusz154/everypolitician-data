{
  "sources": [
    {
      "file": "archive/official-2012-appointed-members.csv",
      "source": "http://www.gov.tc/",
      "generates": "membership"
    },
    {
      "file": "archive/official-2012-elected-members.csv",
      "source": "http://www.gov.tc/",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-2012-elected-members.csv"
      }
    },
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/turks-and-caicos-assembly-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data WHERE term=2012 ORDER BY name"
      },
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikipedia.csv"
      },
      "source": "https://en.wikipedia.org/wiki/Turks_and_Caicos_Islands_general_election,_2012",
      "generates": "membership"
    },
    {
      "file": "morph/wikipedia_2016.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/turks-and-caicos-assembly-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data WHERE term=2016 ORDER BY name"
      },
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikipedia_2016.csv"
      },
      "source": "https://en.wikipedia.org/wiki/Turks_and_Caicos_Islands_general_election,_2016",
      "generates": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/turks-and-caicos-assembly-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Turks-and-Caicos-Islands/Assembly"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q24384357"
      }
    }
  ]
}
