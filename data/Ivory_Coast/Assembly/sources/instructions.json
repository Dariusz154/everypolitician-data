{
  "sources": [
    {
      "file": "morph/abidjan-forums.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/ivory-coast-election-results-2011",
        "query": "SELECT * FROM data ORDER BY name"
      },
      "source": "http://forums.abidjan.net",
      "generates": "membership"
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Ivory-Coast/Assembly"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22342211"
      }
    }
  ]
}
