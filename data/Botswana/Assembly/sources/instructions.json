{
  "sources": [
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/botswana-national-assembly-wp",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id, 11 AS term FROM data ORDER BY name"
      },
      "source": "https://en.wikipedia.org",
      "generates": "membership"
    },
    {
      "file": "archive/wikipedia-ceased.csv",
      "source": "https://en.wikipedia.org",
      "generates": "membership"
    },
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/botswana-national-assembly",
        "query": "SELECT name, email, REPLACE(LOWER(name),' ','-') AS id FROM data ORDER BY name"
      },
      "source": "http://www.parliament.gov.bw/",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/botswana-national-assembly-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "https://www.wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/botswana-parliament-terms-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/botswana-assembly-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22669582"
      }
    }
  ]
}
