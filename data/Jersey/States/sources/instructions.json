{
  "sources": [
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/jersey-assembly",
        "query": "SELECT *, parish AS area FROM data ORDER BY id"
      },
      "source": "http://www.statesassembly.gov.je",
      "generates": "membership"
    },
    {
      "file": "manual/members.csv",
      "source": "http://www.statesassembly.gov.je",
      "generates": "membership",
      "reuse_ids": false
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Jersey/States"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q25343642"
      }
    }
  ]
}
