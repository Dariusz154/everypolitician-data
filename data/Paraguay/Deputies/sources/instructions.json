{
  "sources": [
    {
      "file": "morph/official-2018.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/paraguay-diputados",
        "query": "SELECT *, 2018 AS term FROM data ORDER BY id"
      },
      "source": "http://www.diputados.gov.py",
      "generates": "membership"
    },
    {
      "file": "archive/official-2013.csv",
      "source": "http://www.diputados.gov.py",
      "generates": "membership",
      "reuse_ids": false
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/paraguay-deputies-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/paraguay-deputies-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q25363518"
      }
    }
  ]
}
