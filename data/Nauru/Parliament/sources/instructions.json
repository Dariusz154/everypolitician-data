        "query": "SELECT *, party as faction FROM data"
      },
      "source": "https://en.wikipedia.org/wiki/Parliament_of_Nauru",
      "type": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/nauru-parliament-wikidata",
