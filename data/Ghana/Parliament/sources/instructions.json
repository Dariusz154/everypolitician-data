{
  "sources": [
    {
      "file": "archive/official-term-6.csv",
      "source": "http://www.parliament.gh",
      "generates": "membership"
    },
    {
      "file": "morph/official-term-7.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/ghana-parliament",
        "query": "SELECT *, 7 AS term FROM data ORDER BY id"
      },
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-term-7.csv"
      },
      "source": "http://www.parliament.gh",
      "generates": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/ghana-parliament-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/ghana-parliament-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22283539"
      }
    }
  ]
}
