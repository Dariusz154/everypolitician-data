{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/spain-proyectocolibri",
        "query": "SELECT * FROM data"
      },
      "source": "http://proyectocolibri.es",
      "type": "membership"
    },
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "struan/spain_congreso_es",
        "query": "SELECT *, NULL as dob, NULL AS start_date, NULL AS end_date FROM data WHERE term = '10' ORDER BY id"
      },
      "merge": {
        "incoming_field": "sort_name",
        "existing_field": "sort_name",
        "reconciliation_file": "reconciliation/official.csv"
      },
      "source": "http://www.congreso.es",
      "type": "person"
    },
    {
      "file": "manual/old-official-2015.csv",
      "merge": {
        "incoming_field": "sort_name",
        "existing_field": "sort_name",
        "reconciliation_file": "reconciliation/official-2015.csv"
      },
      "source": "http://www.congreso.es",
      "type": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/spain-diputados-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "wikidata/groups.json",
      "type": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Spain/Congress"
      }
    },
    {
      "file": "wikidata/positions.json",
      "type": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "type": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q17317594"
      }
    }
  ]
}
