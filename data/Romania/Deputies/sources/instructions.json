{
  "sources": [
    {
      "file": "archive/official-7.csv",
      "source": "http://www.cdep.ro/",
      "generates": "membership"
    },
    {
      "file": "morph/official-8.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/romanian-parliament-2016",
        "query": "SELECT *, 8 AS term FROM data ORDER BY id"
      },
      "source": "http://www.cdep.ro/",
      "generates": "membership",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-8.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/romanian-parliament-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "birth_date",
        "existing_field": "birth_date",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/romanian-parliament-terms-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/romania-deputies-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "morph/areas.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/romanian-deputies-constituencies-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "area-wikidata",
      "merge": {
        "reconciliation_file": "reconciliation/areas.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "morph/cabinet.csv",
      "generates": "wikidata-cabinet",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/romania-positions",
        "query": "SELECT * FROM data ORDER BY id, position, start_date"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22275938"
      }
    }
  ]
}
