{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/us-congress-members",
        "query": "SELECT *, CASE WHEN end_date > date('now') THEN '' ELSE end_date END AS end_date FROM data WHERE house = 'sen'"
      },
      "source": "https://github.com/unitedstates/congress-legislators",
      "type": "membership"
    },
    {
      "file": "morph/socialmedia.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/us-congress-members-social-media",
        "query": "SELECT * FROM data"
      },
      "merge": { 
        "incoming_field": "id",
        "existing_field": "id",
        "report_missing": false
      },
      "source": "https://github.com/unitedstates/congress-legislators",
      "type": "person"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/us-senators-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": [{ 
        "incoming_field": "identifier__uscongress",
        "existing_field": "identifier__bioguide"
      },{ 
        "incoming_field": "wikipedia__en",
        "existing_field": "identifier__wikipedia"
      }]
    },
    {
      "file": "ocd/divisions.csv",
      "create": {
        "type": "ocd",
        "source": "country-us.csv"
      },
      "type": "area",
      "generate": "area"
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    }
  ]
}
