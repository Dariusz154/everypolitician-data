{
  "sources": [
    {
      "file": "morph/openaustralia.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/australia-openaustralia-senators",
        "query": "SELECT *, id AS identifier__openaustralia, REPLACE(LOWER(party),' ','_') AS party_id, identifier__aph AS id FROM data ORDER BY id"
      },
      "source": "http://data.openaustralia.org/",
      "generates": "membership"
    },
    {
      "file": "morph/contacts.csv",
      "create": {
        "from": "morph",
        "scraper": "openaustralia/aus_mp_contact_details",
        "query": "SELECT aph_id AS id, full_name AS name, photo_url AS photo FROM data WHERE house = 'senate' ORDER BY aph_id"
      },
      "merge": {
        "incoming_field": "id",
        "existing_field": "identifier__aph"
      },
      "source": "http://data.openaustralia.org/",
      "generates": "person"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/australia-senators-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv",
        "report_extras": false
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/australia-senate-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/twitter.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/twitter-australia-senate",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "person",
      "source": "https://twitter.com/AuSenate/lists/senators",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name__en",
        "reconciliation_file": "reconciliation/twitter.csv"
      }
    },
    {
      "file": "morph/areas.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/australia-states",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "area-wikidata",
      "merge": {
        "reconciliation_file": "reconciliation/areas.csv"
      }
    }
  ]
}
