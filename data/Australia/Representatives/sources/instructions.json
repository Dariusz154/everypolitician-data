{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "type": "morph",
        "scraper": "openaustralia/aus_mp_contact_details",
        "query": "SELECT aph_id AS id, full_name AS name, electorate AS constituency, photo_url AS photo, REPLACE(LOWER(party),' ','_') AS party_id, 44 AS term, * FROM data WHERE house = 'representatives'"
      },
      "source": "https://morph.io/openaustralia/aus_mp_contact_details",
      "type": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/australia-house-of-representatives-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": { 
        "field": "name",
        "overrides": {
        }
      }
    },
    {
      "file": "ocd/divisions.csv",
      "create": {
        "type": "ocd",
        "source": "country-au/federal_electorates.csv"
      },
      "merge": { 
        "overrides": {
          "Fraser, Australian Capital Territory": "ocd-division/country:au/territory:act/federal_electorate:fraser"
        }
      },
      "type": "area"
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    }
  ]
}
