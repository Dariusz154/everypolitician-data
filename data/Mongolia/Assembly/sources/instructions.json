{
  "sources": [
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/mongolia-khurai-wp-multiple-terms",
        "query": "SELECT *, REPLACE(LOWER(constituency),' ','_') AS constituency_id FROM data"
      },
      "source": "https://en.wikipedia.org/",
      "generates": "membership"
    },
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/mongolia-khurai-official",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://www.parliament.mn/",
      "generates": "person",
      "merge": {
        "incoming_field": "name__mn",
        "existing_field": "name__mn",
        "reconciliation_file": "reconciliation/official.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/mongolia-khurai-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name__mn",
        "existing_field": "name__mn",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/mongolia-assembly-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q24566658"
      }
    }
  ]
}
