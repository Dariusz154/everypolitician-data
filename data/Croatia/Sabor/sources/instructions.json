{
  "sources": [
    {
      "file": "morph/sabor-7.csv",
      "source": "http://www.sabor.hr",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/croatia-parliament",
        "query": "SELECT id, name, sortname, image, party, birth_date, constituency, term, start_date, end_date, source FROM data WHERE term = 7"
      },
      "type": "membership"
    },
    {
      "file": "morph/sabor-8.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/croatia-parliament",
        "query": "SELECT id, name, sortname, image, party, birth_date, constituency, term, start_date, end_date, source FROM data WHERE term = 8"
      },
      "source": "http://www.sabor.hr",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/sabor-8.csv"
      },
      "type": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/croatian-parliament-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Croatia/Sabor"
      }
    },
    {
      "file": "wikidata/positions.json",
      "type": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "type": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "type": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22276008"
      }
    }
  ]
}
