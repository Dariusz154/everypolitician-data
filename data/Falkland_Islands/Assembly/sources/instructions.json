{
  "sources": [
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/falkland-islands",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data ORDER BY name"
      },
      "source": "https://en.wikipedia.org/",
      "generates": "membership"
    },
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/falkland-islands-assembly",
        "query": "SELECT REPLACE(LOWER(name),' ','_') AS id, name, image FROM data ORDER BY name"
      },
      "source": "http://www.falklands.gov.fk/self-governance/legislative/assembly-members/",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/falkland-islands-representatives-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Falkland-Islands/Assembly"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q24715318"
      }
    }
  ]
}
