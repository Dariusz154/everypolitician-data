{
  "sources": [
    {
      "file": "morph/oceaniatv.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/palau-2012-election",
        "query": "SELECT * FROM data WHERE won = 'yes' ORDER BY name"
      },
      "source": "http://www.oceaniatv.net/republic-of-palau-2012-elections-candidates/",
      "generates": "membership"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/palau-delegates-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Palau/House-of-Delegates"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q24182685"
      }
    }
  ]
}
