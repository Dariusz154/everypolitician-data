{
  "sources": [
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/tajikistan-parlament",
        "query": "SELECT *, 'Unknown' AS party, 2015 AS term FROM data ORDER BY id"
      },
      "source": "http://www.parlament.tj/",
      "generates": "membership"
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Tajikistan/Representatives"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22333685"
      }
    }
  ]
}
