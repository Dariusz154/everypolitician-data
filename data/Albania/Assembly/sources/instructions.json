{
  "sources": [
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/albania-kuvendi-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data WHERE term = '8' ORDER BY name"
      },
      "source": "https://en.wikipedia.org/",
      "generates": "membership"
    },
    {
      "file": "morph/wikipedia-7.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/albania-kuvendi-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data WHERE term = '7' ORDER BY name"
      },
      "source": "https://en.wikipedia.org/",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikipedia-7.csv"
      },
      "generates": "membership"
    },
    {
      "file": "morph/official-8.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/albania-kuvendi",
        "query": "SELECT id,name,email,image,birth_date,birth_place,source AS website,id AS identifier__kuvendi FROM data ORDER BY id"
      },
      "source": "http://www.parlament.al/",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-8.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/albania-assembly-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/albania-parliament-terms-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/albania-assembly-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22276264"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    }
  ]
}