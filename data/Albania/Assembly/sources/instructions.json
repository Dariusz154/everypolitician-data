{
  "sources": [
    {
      "file": "morph/wikipedia-8.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/albania-kuvendi-wikipedia",
        "query": "SELECT * FROM data"
      },
      "source": "https://en.wikipedia.org/",
      "type": "membership"
    },
    {
      "file": "morph/wikipedia-7.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/albania-assembly-wp",
        "query": "SELECT * FROM data"
      },
      "source": "https://en.wikipedia.org/",
      "type": "membership"
    },
    {
      "file": "morph/official-8.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/albania-kuvendi",
        "query": "SELECT name,image,birth_date,area,faction,term,source AS homepage,id AS identifier__kuvendi FROM data"
      },
      "source": "http://www.parlament.al/",
      "type": "person",
      "merge": { 
        "incoming_field": "name",
        "existing_field": "name",
        "amatch_threshold": 70,
        "amatch_warning": 85
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/albania-assembly-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": { 
        "incoming_field": "name",
        "existing_field": "name",
        "amatch_threshold": 65,
        "amatch_warning": 80
      }
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    }
  ]
}
