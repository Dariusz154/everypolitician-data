{
  "sources": [
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "jennahowe/trinidad_and_tobago",
        "query": "SELECT *, 11 AS term FROM house_of_representatives ORDER BY id"
      },
      "source": "http://ttparliament.org",
      "generates": "membership"
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Trinidad-and-Tobago/Representatives"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q25363097"
      }
    }
  ]
}
