{
  "sources": [
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/saint-lucia-assembly-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','-') AS id FROM data ORDER BY id"
      },
      "source": "https://en.wikipedia.org/",
      "generates": "membership"
    },
    {
      "file": "morph/images.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/saint-lucia-assembly",
        "query": "SELECT *, REPLACE(LOWER(name),' ','-') AS id FROM data ORDER BY id"
      },
      "source": "http://www.govt.lc/house-of-assembly",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/images.csv"
      }
    },
    {
      "file": "archive/official-images-ceased.csv",
      "source": "http://www.govt.lc/house-of-assembly",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-images-ceased.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/saint-lucia-assembly-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Saint-Lucia/Assembly"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q25532438"
      }
    }
  ]
}
