{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/costa-rica-asamblea",
        "query": "SELECT * FROM data"
      },
      "source": "http://www.asamblea.go.cr/",
      "type": "membership"
    },
    {
      "file": "morph/emails.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/costa-rica-asamblea-emails",
        "query": "SELECT * FROM data"
      },
      "type": "person",
      "merge": {
        "incoming_field": "id",
        "existing_field": "id"
      }
    },
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/costa-rica-election-2014",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data"
      },
      "type": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikimedia.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/costa-rica-asamblea-wikidata",
        "query": "SELECT * FROM data"
      },
      "type": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "type": "gender-balance",
        "source": "Costa-Rica/Assembly"
      }
    },
    {
      "file": "wikidata/groups.json",
      "type": "group",
      "create": {
        "type": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/positions.json",
      "type": "wikidata-positions",
      "create": {
        "type": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    }
  ]
}