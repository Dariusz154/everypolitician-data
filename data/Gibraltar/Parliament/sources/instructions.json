{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "type": "morph",
        "scraper": "andylolz/gibraltar-parliament",
        "query": "SELECT * FROM data"
      },
      "source": "http://www.parliament.gi/",
      "type": "membership"
    },
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "type": "morph",
        "scraper": "andylolz/gibraltar-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data"
      },
      "source": "https://en.wikipedia.org/",
      "type": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "term_match": true,
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "type": "morph",
        "scraper": "andylolz/gibraltar-parliament",
        "query": "SELECT * FROM terms"
      },
      "source": "http://www.parliament.gi/",
      "type": "term"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "type": "morph",
        "scraper": "tmtmtmtm/gibraltar-parliament-wikidata",
        "query": "SELECT * FROM data"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": {
        "incoming_field": "wikipedia__en",
        "existing_field": "wikipedia_name"
      }
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "type": "gender-balance",
        "source": "Gibraltar/Parliament"
      }
    }
  ]
}
