{
  "sources": [
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/gibraltar-parliament",
        "query": "SELECT * FROM data"
      },
      "source": "http://www.parliament.gi/",
      "generates": "membership"
    },
    {
      "file": "morph/wikipedia.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/gibraltar-wikipedia",
        "query": "SELECT *, REPLACE(LOWER(name),' ','_') AS id FROM data"
      },
      "source": "https://en.wikipedia.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "term_match": true,
        "reconciliation_file": "reconciliation/wikipedia.csv"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/gibraltar-parliament",
        "query": "SELECT * FROM terms ORDER BY id"
      },
      "source": "http://www.parliament.gi/",
      "generates": "term"
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/gibraltar-parliament-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Gibraltar/Parliament"
      }
    },
    {
      "file": "morph/cabinet.csv",
      "generates": "wikidata-cabinet",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/gibraltar-positions",
        "query": "SELECT * FROM data ORDER BY id, position, start_date"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22704005"
      }
    }
  ]
}
