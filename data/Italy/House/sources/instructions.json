{
  "sources": [
    {
      "file": "morph/camera.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/italy-camera",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://www.camera.it/leg17/313",
      "generates": "membership"
    },
    {
      "file": "morph/openpolis.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/openpolis-scraper",
        "query": "SELECT id,name,surname,term,source,image,email,facebook,twitter, id AS identifier__openpolis FROM data WHERE house = 'Chamber of Deputies' ORDER BY id"
      },
      "source": "http://politici.openpolis.it",
      "generates": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "term_match": true,
        "reconciliation_file": "reconciliation/openpolis.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/italy-legislature-XVII-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "generates": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "morph/cabinet.csv",
      "generates": "wikidata-cabinet",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/italy-positions",
        "query": "SELECT * FROM data ORDER BY id, position, start_date"
      }
    },
    {
      "file": "morph/terms.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/italian-parliament-terms-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "generates": "term"
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/italy-house-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "generates": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "wikidata/positions.json",
      "generates": "wikidata-positions",
      "create": {
        "from": "wikidata-raw",
        "source": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q3722420"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    }
  ]
}
