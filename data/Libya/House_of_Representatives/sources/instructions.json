{
  "sources": [
    {
      "file": "morph/temehu.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/libya-house-of-representatives",
        "query": "SELECT name, area, image, party, term, end_date FROM data ORDER BY name"
      },
      "source": "https://www.temehu.com",
      "generates": "membership"
    },
    {
      "file": "manual/terms.csv",
      "generates": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "generates": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Libya/House-of-Representatives"
      }
    },
    {
      "file": "wikidata/groups.json",
      "generates": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "generates": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22669526"
      }
    }
  ]
}
